{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tracked</h1>
    {% for tracking in trackings %}
        <a href="{{ path('app_tracking_untrack', {'id': tracking.tracked.id}) }}">{{ tracking.tracked.username }}</a>
    {% endfor %}
    <h2>Users</h2>
    {% for user in users %}
        <a href="{{ path('app_tracking_track', {'id': user.id}) }}">{{ user.username }}</a>
    {% endfor %}
{% endblock %}
